+  ASSERT_EQ(long_string, tm->rs(1));
+  assert_nm_on_arena(tm->rs(1));
+  ASSERT_EQ(long_string, tm->rb(0));
+  assert_mn_on_arena(tm->rb(0));
+  ASSERT_EQ(short_string, tm->rb(1));
+  assert_mm_on_arena(tm->rb(1));
+  if (!tarena) {
+    delete tm;
