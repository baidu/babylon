+    uint64_t capacity;
+    char local[16];
+  };
+};
+#else                  // !_GLIBCXX_USE_CXX11_ABI
+struct StdStringRep {
+  uint64_t size;
+  uint64_t capacity;
+  int32_t refcount;
+  uint32_t gap;
+  char data[0];
