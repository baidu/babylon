+  ////////////////////////////////////////////////////////////////////////////
+
+ private:
+  inline internal::StdStringRep& representation() noexcept;
+
+  inline pointer qualified_buffer(size_type required_capacity) noexcept;
+  inline pointer qualified_buffer(size_type required_capacity,
