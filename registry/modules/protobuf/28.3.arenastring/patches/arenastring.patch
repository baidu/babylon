+union StdStringRep {
+  struct {
+    typename ::std::string::size_type capacity;
+    typename ::std::string::size_type size;
+    typename ::std::string::pointer data;
+  } long_format;
+  struct {
+    uint8_t size;
+    typename ::std::string::value_type data[0];
+  } shot_format;
